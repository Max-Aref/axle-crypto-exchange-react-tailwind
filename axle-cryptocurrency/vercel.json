{
  "version": 2,
  "buildCommand": "npm run build",
  "public": true,
  "outputDirectory": "build",
  "env": {
    "REACT_APP_API_URL": "@react_app_api_url",
    "REACT_APP_API_KEY": "@react_app_api_key",
    "REACT_APP_WEBSOCKET_URL": "@react_app_websocket_url"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
